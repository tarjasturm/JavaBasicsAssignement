import java.util.Scanner;

public class Faktoriyel {
	
	


		public static void main(String[] args) {
			
			Scanner deger =new Scanner(System.in);
			
			int a;
			
			System.out.print("faktöriyelini bulmak istediginiz sayiyi giriniz : ");
			
			a = deger.nextInt();
			int i = 1;
			int b=1;
			
			
			while(i<=a) {
				b*=i;
				i++;
				System.out.println(b);
				
			}
			System.out.println();
			System.out.print("cevabınız : "+b);
			
		}

	}


