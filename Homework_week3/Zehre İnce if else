import java.util.Objects;
import java.util.Scanner;

public class Main {
    static Scanner sc = new Scanner(System.in);
    static String isimReel = "Zehre";
    static String soyadReel = "İnce";
    
    public static void main(String[] args)
    {
    	System.out.println("İsim Giriniz: ");
    	String isimln = sc.nextLine();
    	System.out.println("Soyadı giriniz");
    	String soyadln = sc.nextLine();
    	
    	if (Objects.equals(isimln, isimReel)&& Objects.equals(soyadln, soyadReel)) {
    		
    		System.out.println("Girilen bilgiler doğru");
    		
    	}
    	else {
    		System.out.println("İsim ya da soyisim hatalı");
    	}
    }

}
